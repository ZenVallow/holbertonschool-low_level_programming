#include "main.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * main - check the code
 *
 * Return: Always 0.
 */
int main(void)
{
    char *s;

    s = _strdup("Duplicated");
    if (s == NULL)
    {
        printf("failed to allocate memory\n");
        return (1);
    }
    printf("%s\n", s);
    free(s);
    return (0);
}

#include "main.h"

/**
 * _strlen - check the code
 *
 * Return: Always i.
 *
 *@s: string.
 */

int _strlen(char *s)
{
	int i = 0;

	while (s[i] != '\0')
	{

		i++;
	}

	return (i);
}
